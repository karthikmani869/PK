import java.util.LinkedList;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        LinkedList<Integer> k = new LinkedList<Integer>();
        Scanner s = new Scanner(System.in);
        
        int n = s.nextInt(); 
        for (int i = 0; i < n; i++) {
            int val = s.nextInt();
            k.add(val);
        }

        for (int i = 0; i < n; i++) {
            System.out.print(k.get(i));
        }

        s.close();
    }
}
